# For testing HID with no retry

path: /uniqueUsers

properties:
	firstName:
		type: string
		fake: firstName
	lastName:
		type: string
		fake: lastName

fakeDataGenerator:
	type: faker
	locale: fr

slugGeneration:
	properties:
		- firstName
		- lastName
	unicode: true
	retry: true
hidGeneration:
	properties:
		- lastName
		- firstName
	format: %s (%s)
