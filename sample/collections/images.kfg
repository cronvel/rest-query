path: /images

attachmentPath: /images
attachmentPublicBasePath: /images
attachmentHashType: sha256
attachmentAppendExtension: yes

properties:
	name:
		type: string
	file:
		type: attachmentSet
	
	arrayOfAttachments:
		type: array
		default: <Array>
		of:
			type: attachment

	arrayOfAttachmentSets:
		type: array
		default: <Array>
		of:
			type: attachmentSet
