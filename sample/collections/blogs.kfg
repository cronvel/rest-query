path: /blogs
extraProperties: true
properties:
	title:
		type: string
	description:
		type: string
	embedded:
		optional: true
		type: strictObject
	customSchema:
		optional: true
		type: strictObject
alterSchemaProperty: customSchema
hooks: @@blogs.hooks.js 
slugGenerationProperty: title 
autoCollection: posts

# allow filter and sort on unindexed properties
unindexedQueries: true
